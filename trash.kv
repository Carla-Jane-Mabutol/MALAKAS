<Welcome>:

    name: "welcome"
    BoxLayout:
        orientation: "vertical"
        spacing: -60
        BoxLayout:
            orientation: "vertical"
            size_hint: (1, 1)
            Image:
                source: 'header.png'   
                size_hint: (1, 3)
        BoxLayout:
            orientation: 'vertical'
            size_hint: (0.6, 1.5)
            pos_hint: {'center_x': .5, 'center_y': .5}
            spacing: -2  
            Image:
                source: 'logo3.png'
                allow_stretch: True
            Label:
                text: 'Welcome to E - Trash'
                color: 'black'
                bold: 'True'
                font_size: 18
                size_hint: (1, 0.3) 
            Label:
                text: ''
                color: 'black'
                bold: 'True'
                font_size: 14
                size_hint: (1, 0.6)
            Button:
                text: 'RESIDENT'
                on_press: root.manager.current = 'home'
                size_hint: (0.8, 0.45)
                pos_hint: {'center_x': .5, 'center_y': .5}
                background_color: 'blue'
            Label:
                text: 'OR'
                color: 'black'
                bold: 'True'
                font_size: 14
                size_hint: (1, 0.5)
            Button:
                on_press: root.manager.current = 'loginstaff'
                text: 'STAFF'
                size_hint: (0.8, 0.45)
                pos_hint: {'center_x': .5, 'center_y': .5}
                background_color: 'red'
        BoxLayout:
            orientation: 'vertical'
            size_hint: (0.6, 1.2)
            pos_hint: {'center_x': .5, 'center_y': .5}
            spacing: -2  
            Label:
                text: ''
                color: 'black'
                bold: 'True'
                font_size: 14
                size_hint: (1, 1)

